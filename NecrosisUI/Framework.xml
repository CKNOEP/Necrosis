<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<!-- frameStrata Parameters:  BACKGROUND, LOW, MEDIUM, HIGH, DIALOG, FULLSCREEN, FULLSCREEN_DIALOG, TOOLTIP  -->
	<!-- Layer level Parameters:  BACKGROUND, BORDER, ARTWORK, OVERLAY, HIGHLIGHT -->

	<!-- Load NecrosisUI Framework -->
	<Script file="Framework.lua"/>

	<!-- SUI_PartyMemberTemplate -->


	<!-- NUI_AncherFrame -->
	<Frame name="NUI_FramesBase" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT">
				<Offset>
					<AbsDimension x="0" y="-1"/>
				</Offset>
			</Anchor>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="0" y="-1"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>

	<Frame name="NUI_FramesAnchor" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMLEFT" relativeTo="NUI_FramesBase" relativePoint="TOPLEFT" />
			<Anchor point="TOPRIGHT" relativeTo="NUI_FramesBase" relativePoint="TOPRIGHT">
				<Offset>
					<AbsDimension x="0" y="153"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>



</Ui>
